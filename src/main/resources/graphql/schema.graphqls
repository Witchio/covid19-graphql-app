type Query {
  country(name: String!): Country
  countries(names: [String!]!): [Country]
  countriesList: [String]
  maxCases: Country
  minCases: Country
}

type Country {
  countryName: String
  cases: Int
  todayCases: Int
  deaths: Int
  todayDeaths: Int
  recovered: Int
  todayRecovered: Int
}

